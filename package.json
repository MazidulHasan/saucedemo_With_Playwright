{
  "name": "saucedemo-playwright-automation",
  "version": "1.0.0",
  "description": "Enterprise-grade Playwright automation framework for SauceDemo with POM, fixtures, Excel-driven data, and comprehensive reporting",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:smoke": "playwright test --grep @smoke",
    "test:debug": "playwright test --debug",
    "test:report": "playwright test --reporter=html",
    "show:report": "playwright show-report",
    "test:ui": "playwright test --ui"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "saucedemo",
    "pom",
    "fixtures",
    "excel"
  ],
  "author": "QA Automation Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "dependencies": {
    "xlsx": "^0.18.5",
    "fs-extra": "^11.2.0"
  }
}
